{
  "redirects": [
    {
      "source": "https://biovaulthealth.com/:path*",
      "destination": "https://www.biovaulthealth.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/pinealxt", "destination": "/pinealxt.html" },
    { "source": "/moringa", "destination": "/moringa.html" },
    { "source": "/digestsync", "destination": "/digestsync.html" },
    { "source": "/glucotrust", "destination": "/glucotrust.html" },
    { "source": "/prodentim", "destination": "/prodentim.html" },
    { "source": "/gutoptim", "destination": "/gutoptim.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/blog/:path*", "destination": "/blog/:path*" },
    { "source": "/pinealxt-clean-foods", "destination": "/pinealxt-clean-foods.html" },
    { "source": "/pinealxt-clean-foods-opt-in", "destination": "/pinealxt-clean-foods.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/terms-of-service", "destination": "/terms-of-service.html" },
    { "source": "/thank-you", "destination": "/thank-you.html" },
    { "source": "/:path*", "destination": "/:path*" }
  ]
}
